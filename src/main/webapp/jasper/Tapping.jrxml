	<parameter name="@line_id" class="java.lang.String"/>
	<queryString>
		<![CDATA[select distinct   b.map_id,b.area , b.line_id,c.line_name,
 (select area from mms_area where trim(id) = trim(b.area)  ) as  area_name ,
 (select b.comp_nm from Glcompm b where b.comp_Id in ($P!{@province}) ) as province_name,
(select d.line_type from MMS_ADDLINETYPE d where    d.id =  c.line_type  ) as line_type,
b.tower_no,( select d.name from MMS_TOWERTYPE  d where d.id=b.tower_type ) as tower_type, a.NOOFTAPPINGS
from  MMS_TXNTOWERMAINTENANCE  a , MMS_ADDSUPPORT  b , MMS_ADDLINE c
where      trim(a.towerid) = trim(b.id) and  trim(b.line_id)  = trim(c.id)
and b.area like   $P!{@area}||'%' and b.line_id  like $P!{@line_id}||'%'
and trim(b.area) in (select a.dept_id from Gldeptm a where a.comp_Id in (select b.comp_id from Glcompm b where b.parent_Id in( $P!{@province}) ) )
and  a.NOOFTAPPINGS >0
and  b.status=1 and  c.status=1 and  a.status=1
group by b.area , b.line_id,c.line_name, b.map_id, b.tower_no ,c.line_type, b.tower_type, a.NOOFTAPPINGS
order by   b.area , b.line_id ,  b.map_id,b.tower_no]]>
