	<queryString>
		<![CDATA[select b.map_id, b.line_id, (select b1.line_name from mms_Addline b1 where b1.id= b.line_id ) as line_name, b.tower_no,b.area ,a.towerspecial ,a.HOT_LINE_MNT,  (select dept_nm from Gldeptm a where a.dept_id=b.area   ) as  area_name,
 (select b.comp_nm from Glcompm b where b.comp_Id in ( $P!{@province}) ) as province_name,
 (select b1.line_name from MMS_ADDLINE b1 where b1.Id =b.line_id ) as line_name
 from  MMS_TXNTOWERMAINTENANCE  a , MMS_ADDSUPPORT  b
where a.towerid = b.id
and  a.towerspecial !='None'
and b.area like   $P!{@area}||'%' and b.line_id  like $P!{@line_id}||'%'
and   (( a.visitid    like $P!{@cycle}||'%' ) or (  a.visitid   = $P!{@cycle}    and  $P!{@cycle} in ('201901') ) or ( $P!{@cycle}  ='201801'    and   a.visitid not in  ('201901')))
and trim(b.area) in (select a.dept_id from Gldeptm a where a.comp_Id in (select b.comp_id from Glcompm b where b.parent_Id in( $P!{@province}) ) )
 and  b.status=1  and  a.status=1
group by b.area , b.line_id,b.line_name,b.map_id, b.tower_no,a.towerspecial,a.HOT_LINE_MNT
order by b.area , b.line_id ,b.map_id, b.tower_no]]>
