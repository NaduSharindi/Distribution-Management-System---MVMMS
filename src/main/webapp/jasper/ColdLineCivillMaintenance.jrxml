 (select d.line_type from MMS_ADDLINETYPE d where    d.id =  c.line_type  ) as line_type, a.BASECONCRETESTATUS,a.ANTICLIMBINGSTATUS, a.NOOFMISSINGPARTS as Missing_part,a.LEG_PAINTING,
 (select area from mms_area where trim(id) = trim(b.area)  ) as  area_name
 from  MMS_TXNTOWERMAINTENANCE  a , MMS_ADDSUPPORT  b , MMS_ADDLINE c
where     b.TOWER_CONFIGURATION in (1,2)
and b.area like   $P!{@area}||'%' and b.line_id  like $P!{@line_id}||'%'
and trim(a.towerid) = trim(b.id) and  trim(b.line_id)  = trim(c.id)
and  ( (a.BASECONCRETESTATUS  <>'Good' ) OR  (a.ANTICLIMBINGSTATUS <>'Good')  OR   (a.NOOFMISSINGPARTS !=0 ) OR (a.LEG_PAINTING   <>'No' and  a.LEG_PAINTING  != '0'))
group by b.area , b.line_id,b.line_name, b.TOWER_CONFIGURATION,  b.tower_no ,b.tower_type ,c.line_type , a.BASECONCRETESTATUS,a.ANTICLIMBINGSTATUS,a.NOOFMISSINGPARTS,a.LEG_PAINTING
