	<queryString>
		<![CDATA[select distinct b.line_id,b.line_name, b.TOWER_CONFIGURATION,NOOFTAPPINGS,(select d.NAME from MMS_ADDTOWERCONFIGURATION d where  b.TOWER_CONFIGURATION= d.id) as TOWER_CONFIGURATION_N,  b.tower_no,b.area ,( select d.name from MMS_TOWERTYPE  d where d.id=b.tower_type ) as tower_type , (select d.line_type from MMS_ADDLINETYPE d where    d.id =  c.line_type  ) as line_type,a.NOFFLASHOVERSETS, a.FUNGUSSETNO, a.WPINSET,a.ENDFITTINGSET,
a.JUMPERSTATUS,a.CONDUCTORSTATUS, a.EARTHCONDUCTORSTATUS,(Case when a.HOTPOSSIBLE=1 then 'Possible'   when a.HOTPOSSIBLE=0 then 'Impossible' else '' end) as Hotpossible ,
 (select area from mms_area where trim(id) = trim(b.area)  ) as  area_name
 from  MMS_TXNTOWERMAINTENANCE  a , MMS_ADDSUPPORT  b , MMS_ADDLINE c
where    ( ( b.TOWER_CONFIGURATION in (3) and b.support_type =1 ) or  (b.support_type =2 ))
and b.area like   $P!{@area}||'%' and b.line_id  like $P!{@line_id}||'%'
and trim(a.towerid) = trim(b.id) and  trim(b.line_id)  = trim(c.id)
and ( (a.NOFFLASHOVERSETS > 0)  or ( a.FUNGUSSETNO > 0) or ( a.WPINSET > 0)or ( a.ENDFITTINGSET > 0) )
and ( a.JUMPERSTATUS !='Good' or a.JUMPERSTATUS !='None' or a.CONDUCTORSTATUS !='Good' or  a.EARTHCONDUCTORSTATUS  !='Good' or  a.EARTHCONDUCTORSTATUS  !='None' or  a.towerspecial !='None'  or a.LEG_PAINTING   !='None'
 or a.NOOFMISSINGPARTS !=0 or a.WAYLEAVESTATUS !='Good' or a.BASECONCRETESTATUS !='Good' or a.ANTICLIMBINGSTATUS !='Good' )
group by b.area , b.line_id,b.line_name, b.TOWER_CONFIGURATION,NOOFTAPPINGS, b.tower_no,a.towerspecial, b.tower_type, c.line_type,a.NOFFLASHOVERSETS, a.FUNGUSSETNO, a.WPINSET,a.ENDFITTINGSET,a.JUMPERSTATUS,a.CONDUCTORSTATUS, a.EARTHCONDUCTORSTATUS, a.HOTPOSSIBLE
order by b.area , b.line_id ,  b.TOWER_CONFIGURATION ,  b.tower_no,NOOFTAPPINGS]]>
