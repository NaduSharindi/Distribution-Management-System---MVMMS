	<queryString>
		<![CDATA[select distinct b.line_id,b.line_name,
 (select area from mms_area where trim(id) = trim(b.area)  ) as  area_name ,
(select d.line_type from MMS_ADDLINETYPE d where    d.id =  c.line_type  ) as line_type,
b.tower_no,( select d.name from MMS_TOWERTYPE  d where d.id=b.tower_type ) as tower_type, a.NOOFMISSINGPARTS
from  MMS_TXNTOWERMAINTENANCE  a , MMS_ADDSUPPORT  b , MMS_ADDLINE c
where      trim(a.towerid) = trim(b.id) and  trim(b.line_id)  = trim(c.id)
and  a.NOOFMISSINGPARTS >0
and b.area = $P!{@area} and b.line_id  like $P!{@line_id}||'%'
and a.MAINTENANCEDATE >= TO_DATE($P!{@fromDate},'yyyy/mm/dd')
and a.MAINTENANCEDATE<= TO_DATE($P!{@toDate},'yyyy/mm/dd')
order   by b.line_id , b.tower_no]]>
	</queryString>
