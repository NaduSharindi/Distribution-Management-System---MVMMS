	<queryString>
		<![CDATA[select distinct b.map_id,b.line_id,c.line_name, b.TOWER_CONFIGURATION,NOOFTAPPINGS,(select d.NAME from MMS_ADDTOWERCONFIGURATION d where  b.TOWER_CONFIGURATION= d.id) as TOWER_CONFIGURATION_N,  b.tower_no,b.area ,( select d.name from MMS_TOWERTYPE  d where d.id=b.tower_type ) as tower_type , (select d.line_type from MMS_ADDLINETYPE d where    d.id =  c.line_type  ) as line_type,a.NOFFLASHOVERSETS, a.FUNGUSSETNO, a.WPINSET,a.ENDFITTINGSET,
a.JUMPERSTATUS,a.CONDUCTORSTATUS, a.EARTHCONDUCTORSTATUS,(Case when a.HOTPOSSIBLE=1 then 'Possible'   when a.HOTPOSSIBLE=0 then 'Impossible' else '' end) as Hotpossible ,
 (select dept_nm from Gldeptm a where a.dept_id=b.area   ) as  area_name,
 (select b.comp_nm from Glcompm b where b.comp_Id in ($P!{@province}) ) as province_name
 from  MMS_TXNTOWERMAINTENANCE  a , MMS_ADDSUPPORT  b , MMS_ADDLINE c
where
( ( b.TOWER_CONFIGURATION in (1,2) and    NOOFTAPPINGS =0  and a.HOTPOSSIBLE =0 ) OR
( b.TOWER_CONFIGURATION in (1,2) and    NOOFTAPPINGS >0) OR
 (b.TOWER_CONFIGURATION in (1) and    NOOFTAPPINGS =0   and a.HOTPOSSIBLE =0 ))
and trim(a.towerid) = trim(b.id) and  trim(b.line_id)  = trim(c.id)
and ( (a.NOFFLASHOVERSETS > 0)  or ( a.FUNGUSSETNO > 0) or ( a.WPINSET > 0)or ( a.ENDFITTINGSET > 0) )
and ( a.JUMPERSTATUS !='Good' or a.JUMPERSTATUS !='None' or a.CONDUCTORSTATUS !='Good' or  a.EARTHCONDUCTORSTATUS  !='Good' or
 a.EARTHCONDUCTORSTATUS  !='None' )
and b.area like   $P!{@area}||'%' and b.line_id  like $P!{@line_id}||'%'
and   (( a.visitid    like $P!{@cycle}||'%' ) or (  a.visitid   = $P!{@cycle}    and  $P!{@cycle} in ('201901') ) or ( $P!{@cycle}  ='201801'    and   a.visitid not in  ('201901')))
and trim(b.area) in (select a.dept_id from Gldeptm a where a.comp_Id in (select b.comp_id from Glcompm b where b.parent_Id in( $P!{@province}) ) )
 and  b.status=1 and  c.status=1 and  a.status=1
group by  b.area ,b.line_id,c.line_name,b.map_id, b.TOWER_CONFIGURATION,NOOFTAPPINGS, b.tower_no,a.towerspecial, b.tower_type, c.line_type,a.NOFFLASHOVERSETS, a.FUNGUSSETNO, a.WPINSET,a.ENDFITTINGSET,a.JUMPERSTATUS,a.CONDUCTORSTATUS, a.EARTHCONDUCTORSTATUS, a.HOTPOSSIBLE
order by b.area , b.line_id , b.map_id, b.TOWER_CONFIGURATION ,  b.tower_no,NOOFTAPPINGS]]>
	</queryString>
